<Page x:Class="Brusnika.Views.Pages.MenuPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Brusnika.Views.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="700" d:DesignWidth="1000"
      Title="MenuPage">

    <Grid Background="#D4B9BB">
        <ScrollViewer>
            <StackPanel Width="600">
                <Image Source="/Resources/menu1.png"/>
                <Image Source="/Resources/menu2.png"/>
                <Image Source="/Resources/menu3.png"/>
                <Image Source="/Resources/menu4.png"/>
                <Image Source="/Resources/menu5.png"/>
                <Image Source="/Resources/menu6.png"/>
            </StackPanel>
        </ScrollViewer>
        <Button Name="BackBtn" Content="Назад" Click="BackBtn_Click" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="20" Width="150"/>
    </Grid>
</Page>
